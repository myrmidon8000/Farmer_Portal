package com.lti.dao;

import org.slf4j.Logger;
import org.slf4j.LoggerFactory;
import org.springframework.stereotype.Repository;

import com.lti.model.FarmerDetails;
@Repository
public class FarmerDetailsDaoImpl implements IFarmerDetailsDao {
	private static final Logger logger = 			
			LoggerFactory.getLogger(FarmerDaoImpl.class);
	public void addFarmerDetails(FarmerDetails farmerDetails) {
		Session session = this.sessionFactory.openSession();
		session.save(farmerDetails);
		logger.info("Employee details saved successfully as: "+farmerDetails);
		session.close();
		
		
	}
	

}
